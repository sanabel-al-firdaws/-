{
  "mobilePullAction": "obsidian-git:push",
  "livePreview": false,
  "rightToLeft": true,
  "mobileToolbarCommands": [
    "editor:undo",
    "editor:redo",
    "editor:toggle-code",
    "editor:toggle-highlight",
    "editor:attach-file",
    "editor:toggle-checklist-status",
    "font-size:decrement-font-size",
    "font-size:increment-font-size",
